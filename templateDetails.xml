<extension version="2.5" type="template" client="site">
	<name>geolive</name>
	<author>Emily Millard</author>
	<authorEmail>emilyloumillard@gmail.com</authorEmail>
	<authorUrl>geolive.ca</authorUrl>
	<copyright>Copyright (C) 2005 - 2013 Nick Blackwell Software, Inc. All rights reserved.</copyright>
	<license>GNU General Public License version 2 or later; see LICENSE.txt</license>
	<version>2.5.0</version>
	<description>Responsive Template for Joomla</description>

	<files>
		<folder>css</folder>
		<folder>images</folder>
		<folder>fonts</folder>
		<folder>js</folder>
		<filename>index.php</filename>
		<filename>templateDetails.xml</filename>
